{ "version": 2, 
"rewrites": [{ "source": "/(.*)", "destination": "/api" }],
"buildCommand": "next build",
"cleanUrls": true,
"crons": [
    {
      "path": "/every-minute",
      "schedule": "* * * * *"
    },
    {
      "path": "/every-hour",
      "schedule": "0 * * * *"
    },
    {
      "path": "/every-day",
      "schedule": "0 0 * * *"
    }
  ],
  "devCommand": "next dev"
 }